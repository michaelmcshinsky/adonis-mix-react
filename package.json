{
  "name": "adonis-mix-react",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node build/server.js",
    "server": "node ace serve --watch",
    "client": "node ace mix:watch",
    "build": "yarn client:build && yarn server:build",
    "server:build": "node ace build --production",
    "client:build": "node ace mix:build --production",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@adonisjs/assembler": "^3.0.0",
    "@babel/preset-react": "^7.12.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "adonis-preset-ts": "^2.0.0",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "concurrently": "^5.3.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-adonis": "^1.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "laravel-mix": "^6.0.11",
    "laravel-mix-tailwind": "^0.1.2",
    "pino-pretty": "^4.5.0",
    "postcss": "8.1",
    "prettier": "^2.2.1",
    "react-refresh": "^0.9.0",
    "sass": "^1.32.7",
    "sass-loader": "8.*",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.5",
    "youch": "^2.1.1",
    "youch-terminal": "^1.0.1"
  },
  "dependencies": {
    "@adonisjs/core": "^5.0.4-preview-rc",
    "@adonisjs/repl": "^1.0.0",
    "@adonisjs/session": "^4.0.0",
    "@adonisjs/view": "^3.0.0",
    "adonis-mix-asset": "^2.2.1",
    "proxy-addr": "^2.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
